  "scripts": {
    "start": "npx nodemon -V -L -w ${ROOT_DIR} --ignore ${ROOT_DIR}/lib/public ${ROOT_DIR}/server",
    "start0": "node ${ROOT_DIR}/server",
    "test": "lab -a @hapi/code -I \"__core-js_shared__,CSS,regeneratorRuntime,core\" -L",
    "test0": "lab -a @hapi/code -L",
    "lint": "eslint ${ROOT_DIR}/.",
    "prebuild:css": "sass ${ROOT_DIR}/lib/public/css/main.scss ${ROOT_DIR}/lib/public/css/main.build.css --source-map",
    "-prebuild:css": "node-sass lib/public/css/main.scss ${ROOT_DIR}/lib/public/css/main.build.css --source-map true",
    "build:css": "postcss ${ROOT_DIR}/lib/public/css/main.build.css -o ${ROOT_DIR}/lib/public/css/main.build.min.css --use cssnano --no-map",
    "prebuild:js": "browserify ${ROOT_DIR}/lib/public/js/main.js -o ${ROOT_DIR}/lib/public/js/main.build.js -d -t [ babelify --presets [ @babel/preset-env ] ]",
    "build:js": "uglifyjs ${ROOT_DIR}/lib/public/js/main.build.js -o ${ROOT_DIR}/lib/public/js/main.build.min.js",
    "build": "npm run build:css && npm run build:js"
  },
